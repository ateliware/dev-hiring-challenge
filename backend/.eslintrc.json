{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-param-reassign": "off",
		"no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
		"camelcase": "off",
		"class-methods-use-this": "off",
		"no-extra-boolean-cast": "off"
	}
}
